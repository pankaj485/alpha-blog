<h1>create new article</h1>
<%= render  "error"  %>
<%= form_with scope: :article, url: articles_path, local:true, data: { turbo: false } do |f|  %>
  <p>
    <%= f.label :title, "title"  %>
    <br>
    <%= f.text_field :title  %>
  </p>
  <p>
    <%= f.label :description, "description"  %>
    <br>
    <%= f.text_area :description  %>
  </p>
  <p>
    <%= f.submit  %>
  </p>
<% end  %>
<%= link_to "Return to articles list", articles_path  %>
